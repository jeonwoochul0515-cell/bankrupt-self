{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "bankrupt-self.vercel.app" }
      ],
      "destination": "https://busan-hoiseng.pro/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/admin", "destination": "/admin.html" }
  ]
}
